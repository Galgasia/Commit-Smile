{
	"name": "commitsmile",
	"version": "0.3.2",
	"description": "Make smile on your commits",
	"keywords": [],
	"license": "ISC",
	"author": "ineedjobtostartwork",
	"type": "module",
	"main": "index.js",
	"bin": {
		"commitsmile": "./dist/main.cjs"
	},
	"scripts": {
		"build": "webpack --config ./config/webpack/webpack.prod.js",
		"dev": "webpack --config ./config/webpack/webpack.dev.js",
		"format": "pnpm prettier . --write && pnpm prettier . --check",
		"link-cli": "pnpm unlink --global && pnpm link --global",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"pre": "git add . && npx lint-staged",
		"prepare": "husky install",
		"start": "node ./dist/main.cjs",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"lint-staged": {
		"*": [
			"git add .",
			"pnpm format",
			"pnpm lint:fix",
			"pnpm lint"
		]
	},
	"dependencies": {
		"commander": "^11.1.0",
		"consola": "^3.2.3",
		"jiti": "^1.21.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@stylistic/eslint-plugin": "^1.5.1",
		"@swc/cli": "^0.1.63",
		"@swc/core": "^1.3.101",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/node": "^20.10.6",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"chalk": "^5.3.0",
		"eslint": "^8.56.0",
		"eslint-plugin-optimize-regex": "^1.2.1",
		"eslint-plugin-pii": "^1.0.2",
		"eslint-plugin-security": "^2.1.0",
		"eslint-plugin-sort-keys-fix": "^1.1.2",
		"globals": "^13.24.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1",
		"prettier-plugin-packagejson": "^2.4.8",
		"prettier-plugin-sort-json": "^3.1.0",
		"swc-loader": "^0.2.3",
		"typescript": "^5.3.3",
		"vitest": "^1.1.3",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"webpack-merge": "^5.10.0"
	},
	"packageManager": "pnpm@8.11.0",
	"engines": {
		"node": ">=18"
	}
}
